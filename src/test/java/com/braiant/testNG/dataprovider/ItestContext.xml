<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite IText">
    <test name="Group Smoke test">
        <groups>
            <run>
                <include name="Smoke"/>
            </run>
        </groups>
        <classes>
            <class name="com.braiant.testNG.dataprovider.ItestContextExample"></class>
        </classes>
    </test>
    <test name="Group Regression test">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>
        <classes>
            <class name="com.braiant.testNG.dataprovider.ItestContextExample"></class>
        </classes>
    </test>

</suite>